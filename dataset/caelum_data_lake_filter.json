{
	"name": "caelum_data_lake_filter",
	"properties": {
		"linkedServiceName": {
			"referenceName": "datalake",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"country": {
				"type": "string"
			},
			"zone": {
				"type": "string"
			},
			"source": {
				"type": "string"
			},
			"table": {
				"type": "string"
			},
			"adls_dw_container_name": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "Parquet",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"fileName": "*.parquet",
				"folderPath": {
					"value": "@concat('dw_data_store/',dataset().zone,'/',dataset().country,'/',dataset().source,'/',dataset().table,'/*/*/*')",
					"type": "Expression"
				},
				"fileSystem": {
					"value": "@dataset().adls_dw_container_name",
					"type": "Expression"
				}
			},
			"compressionCodec": "none"
		},
		"schema": []
	},
	"type": "Microsoft.DataFactory/factories/datasets"
}